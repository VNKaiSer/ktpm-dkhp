generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model course {
  id           String @id @default(auto()) @map("_id") @db.ObjectId
  Prerequisite String
  codeCourse   String
  codeSub      String
  credits      Int
  name         String
  pracNum      Int
  theoryNum    Int
}

model studyProgram {
  id              String   @id @default(auto()) @map("_id") @db.ObjectId
  /// Multiple data types found: Array(String): 11.1%, Array(String (ObjectId)): 88.9% out of 9 sampled entries
  electiveCourses String[]
  requiredCourses String[]
  semester        String
  totalCredits    Int
}